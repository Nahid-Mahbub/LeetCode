class Solution:
    def addToArrayForm(self, num: list[int], k: int) -> list[int]:
        result = []
        for i in range (len(num)-1, -1, -1):
            k += num[i]
            result.append(k % 10)
            k //=10
        return result[::-1]

solution = Solution()
num = [1,2,0,0]
k = 34
result = solution.addToArrayForm(num, k)
print(result)
